	.MODEL	small
	.DATA
B_1	dw	?
C_1	dw	?
D_1	dw	?
X	dw	?
Y	dw	?
N	dw	?
M	dw	?
R1	dw	?
Const1	dw	3
Const2	dw	6
Const3	dw	1
Const4	dw	5
Const5	dw	9
Const6	dw	8
Const7	dw	9
Const8	dw	10
Const9	dw	2
	.CODE
PROC	ZZ
	MOV	AX,Const1
	MOV	C_1,AX
	MOV	AX,Const7
	MOV	B_1,AX
	MOV	AX,Const8
	MOV	D_1,AX
	RET
ENDP	ZZ
Main:
	MOV	AX,Const3
	MOV	N,AX
	MOV	AX,Const3
	SUB	AX,M
	MOV	R1,AX
	MOV	AX,Const9
	ADD	AX,R1
	MOV	R1,AX
	MOV	AX,R1
	MOV	M,AX
	MOV	AX,Const3
	SUB	AX,M
	MOV	R1,AX
	MOV	AX,Const9
	SUB	AX,R1
	MOV	R1,AX
	MOV	AX,R1
	MOV	N,AX
	CALL	ZZ
	MOV	AX,M
	ADD	AX,Const4
	MOV	R1,AX
	MOV	AX,R1
	MOV	X,AX
	MOV	AX,Const5
	CMP	N,AX
	JNL	A1
	MOV	AX,N
	ADD	AX,Const1
	MOV	R1,AX
	MOV	AX,R1
	MOV	Y,AX
	JMP	A2
A1:
	MOV	AX,Const6
	MOV	Y,AX
A2:
	MOV	AX,R1
	MOV	X,AX
END	Main
